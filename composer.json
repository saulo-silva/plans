{
    "name": "saulo-silva/plans",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "fourstacks/nova-checkboxes": "^0.1.5",
        "vyuldashev/nova-money-field": "^1.3",
        "owenmelbz/nova-radio-field": "^0.0.5"
    },
    "autoload": {
        "psr-4": {
            "SauloSilva\\Plans\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SauloSilva\\Plans\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
